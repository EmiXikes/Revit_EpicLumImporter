<UserControl x:Class="EpicLumImporter.UI.View.RvtFamSelectorItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EpicLumImporter.UI.View"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="300">
    <DockPanel >
        <TextBlock Text="{Binding RevitFamilyManufacturer}" Width="80" VerticalAlignment="Center"/>
        <TextBlock Text="{Binding RevitFamilyName}" VerticalAlignment="Center"/>

    </DockPanel>
</UserControl>
